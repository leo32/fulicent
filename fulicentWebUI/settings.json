{
    "eslint.validate": [
      "javascript",
      "javascriptreact",
      "html",
      { "language": "vue", "autoFix": true }
    ],
    "eslint.options": {
      "plugins": ["html"]
    },
    "window.zoomLevel": 0,
    "editor.detectIndentation": false,
    "vetur.format.defaultFormatter.html": "js-beautify-html",
    "editor.formatOnSave": true,
    "prettier.eslintIntegration": true,
    "vetur.format.defaultFormatterOptions": {
      "prettier": {
        "singleQuote": true,
        "semi": false
      }
    }
  }